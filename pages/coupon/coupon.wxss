.u-search-box text{font-size: 28rpx;color: #d5d5d5}
.m-search-box{padding:20rpx;background-color: #EFEFF4;overflow: hidden}
.u-input-search {width: 100%;height: 70rpx;background-color: white;
float: left;border-radius: 8rpx;overflow: hidden;border: 1rpx solid #DFDFDF;}
.u-input-search input{font-size: 28rpx;height: 100%;width: 97%;float: left;padding-left:20rpx;
}
.u-paa{width: 100%;z-index: 5;height: 100%;background-color: #f8f8f8;}
.u-paa .m-product-list{padding-top: 68rpx;}
.m-product-list{background-color: f8f8f8 !important;}
.m-product-item{height: 212rpx;overflow: hidden;background-color: #fff;}
.m-product-img{width: 212rpx;height: 212rpx;float: left;text-align: center;}
.m-product-img image{width:100%;height:100%;box-sizing: border-box;padding: 20rpx;}
.m-product-info{height: 210rpx;width:538rpx;float: left;box-sizing: border-box;padding:20rpx 0;position: relative;}
.m-product-info::before {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1rpx;border-bottom: 1rpx solid #d5d5d5;color: #CCCCCC;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);z-index: 3;}
.m-product-item:last-child .m-product-info::before{content: "";border: none}
.m-product-name{font-size: 28rpx;line-height: 40rpx;height: 130rpx;box-sizing: border-box;padding-right:20rpx;position: relative;}
