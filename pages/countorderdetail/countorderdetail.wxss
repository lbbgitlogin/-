.m-cells-wrap{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx 30rpx
}
.m-cells-time{
  color: rgb(102, 102, 102);
  margin-top: 10rpx
}
.m-cells-right{
  color: #db384c
}
.content{
  position: relative;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  height: 500rpx
}
.content text{
  margin-top: 60rpx;
}
.detail{padding: 20rpx 30rpx 0 30rpx;font-size: 28rpx;color: #666;background: #fff;position: relative}
.detail::before {content:" ";position: absolute;left: 0;top: 0;right: 0;height: 1px;border-top: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.detail::after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.detail-center{display: flex;justify-content: space-between}
.m-total-info{padding: 24rpx 0rpx;margin-top:20rpx;display: flex;justify-content: space-between;align-items: center;background: #fff;position: relative}
.m-total-info::after {content: " ";position: absolute;left: 0;top: 0;right: 0;height: 1px;border-top: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.m-total-info label{color: #DB384C;}