.currentFG{padding:20rpx;background-color: #fff;margin-top: 20rpx;text-align: center;}
.currentFG-item {display: inline-block;width: 100rpx;height: 100rpx;border-radius: 100%;overflow: hidden;}
.currentFG-item image{width: 100%;height: 100%;}
/*当前拼团*/
.u-tap-btn{position: fixed;right: 20rpx;bottom: 80rpx;z-index: 10;}
.u-go-home{border-radius: 100%;width: 80rpx;height: 80rpx;border: 1px solid #eee;font-size: 20rpx;text-align: center;background-color: #fff;box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.35);z-index: 10;line-height:80rpx; margin-bottom: 20rpx;}
.u-go-home .iconfont{font-size: 40rpx;}