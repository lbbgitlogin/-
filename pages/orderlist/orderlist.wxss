.msk{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4)
}
.msk-wrap{
  width: 25%;
  background: #fff;
  border-radius: 10rpx;
  position: fixed;
  right: 10rpx;
  top: 84rpx
}
.msk-item{
  font-size: 24rpx;
  color: #999;
  padding: 20rpx 20rpx;
  position: relative;
  text-align: center
}
.msk-item1{
  font-size: 24rpx;
  color: rgb(40, 147, 228);
  padding: 20rpx 20rpx;
  position: relative;
  text-align: center
}
.msk-item:after,.msk-item1:after {content: " ";position: absolute;left: 20rpx;bottom: 0;right: 20rpx;height: 1px;border-bottom: 1px solid #CCCCCC;color: #CCCCCC;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);z-index: 3;} 
.msk1{
