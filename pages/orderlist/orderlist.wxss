}
.model{
  width: 80%;
  height: 458rpx;
  background: #fff;
  border-radius: 10rpx;
  margin: auto;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0
}
.title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #2893e4;
  height: 80rpx;
  color: #fff;
  padding: 0 30rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.title image{
  width: 30rpx;
  height: 30rpx
}
.content{
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  height: 248rpx;
  position: relative;
}
.content-center:nth-child(2){
  margin-top: 20rpx
}
.content input{
  width: 98%;
  height: 60rpx;
  background: #f7f7f7;
  border-radius: 8rpx;
  margin-top: 10rpx;
  padding-left: 2%;
  font-size: 24rpx
}
.content::after{content: ''; position: absolute; top: 0; left: 0; border-bottom: 1px solid #e5e5e5; -webkit-box-sizing: border-box; box-sizing: border-box; width: 200%; height: 200%; -webkit-transform: scale(0.5); transform: scale(0.5); -webkit-transform-origin: left top; transform-origin: left top; }
.operation{
  height: 90rpx;
  display: flex;
  align-items: center
}
.cancel,.success{
  flex: 1;
  background: #fff;
  font-size: 36rpx;
  text-align: center;
}
.cancel{
  position: relative;
  line-height:90rpx;
  border-bottom-left-radius:8rpx;
}
.cancel::after{content: ''; position: absolute; top: 0; left: 0; border-right: 1px solid #e5e5e5; -webkit-box-sizing: border-box; box-sizing: border-box; width: 200%; height: 200%; -webkit-transform: scale(0.5); transform: scale(0.5); -webkit-transform-origin: left top; transform-origin: left top; }
.success{
  color: #2893e4;
  padding: 0
}
