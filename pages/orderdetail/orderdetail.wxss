.m-cells-wrap{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx 30rpx
}
.m-cells-time{
  color: rgb(102, 102, 102);
  margin-top: 10rpx
}
.m-cells-right{
  color: #db384c
}
.logistics-wrap{
  padding: 20rpx 30rpx;
  position: relative;
  background: #fff;
  font-size: 28rpx
}
.logistics-wrap:after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.logistics{display: flex;justify-content: space-between}
.logistics-wrap{
  padding: 20rpx 30rpx;
  position: relative;
  background: #fff;
  font-size: 28rpx;
}
.logistics-wrap:after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
/* .logistics-num{margin: 10rpx 0} */
.logistics{margin-bottom: 10rpx}
.logistics:last-child{margin-bottom: 0}
.logistics-message{position: relative}
.m-cell-ft:after {content: " ";display: inline-block;height: 12rpx;width: 12rpx;border-width: 4rpx 4rpx 0 0;border-color: #C8C8CD;border-style: solid;-webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);top: -4rpx;position: absolute;top: 50%;margin-top: -7rpx;right: 0rpx;}
.m-addres-info {padding: 20rpx 30rpx;background-color: white;overflow: hidden;position: relative;margin-top: 20rpx;}
.m-addres-info:after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.m-ad-name{display: flex;font-weight: 400;font-size: 14px;justify-content: space-between}
.m-ad-address{width: 650rpx;margin-top: 20rpx;display: -webkit-box;
-webkit-box-flex: 1;line-height: 40rpx;font-weight: 400;
font-size: 14px;position: relative;padding-left: 40rpx;
overflow:hidden;text-overflow:ellipsis;display:-webkit-box;
-webkit-box-orient:vertical;-webkit-line-clamp:2;color: #888;}
/* .m-ad-address::before{content: "\e66c";position: absolute;width: 30rpx;height: 30rpx;font-family:"iconfont" !important;text-align: center;line-height: 35rpx;font-size: 24rpx;background-size: cover;left: 0} */
.m-ad-address image{position: absolute;width: 30rpx;height: 36rpx;left: 0}
.m-product-item{background-color:#fafafa}
.m-product-price{display: flex;justify-content: flex-start} 
.m-product-price image{width: 60rpx;height: 36rpx}
.m-product-name{display: flex;}
.product-name{width: 75%;}
.product-price{width: 25%;-webkit-line-clamp:1;}
.product-price label{text-align: right;height: 42rpx}
.remark{background: rgb(255, 249, 230);color: rgb(51, 51, 51);padding: 20rpx 30rpx;font-size: 24rpx;height: 110rpx}
.conter{margin-top: 10rpx}
.detail{padding: 20rpx 30rpx;font-size: 28rpx;color: #666;background: #fff;position: relative}
.detail::before {content:" ";position: absolute;left: 0;top: 0;right: 0;height: 1px;border-top: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.detail::after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.detail-center{display: flex;justify-content: space-between}
.m-total-info{padding: 24rpx 30rpx;font-size: 24rpx;display: flex;justify-content: space-between;align-items: center;background: #fff;position: relative}
.m-total-info::after {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 1px;border-bottom: 1px solid #D9D9D9;color: #D9D9D9;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.m-total-info label{font-size: 32rpx;color: #DB384C;margin-right: 8rpx}
.m-total-btn{text-align: right;position:fixed;bottom:0;background:#fff;width:100%;}
.m-total-btn .u-link-btn{margin: 20rpx 24rpx 20rpx 0rpx;vertical-align:middle;display:inline-block;line-height:40rpx;background-color: #fff;color: #2991e3;border: 1rpx solid #2991e3;border-radius: 8rpx}
.img_text{
 align-items: center;display: flex;
}
.img_tel{
  width:30rpx;
  height:30rpx;
  margin-right:10rpx
}